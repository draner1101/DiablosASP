@model Diablos_Archives.Model.JoueurViewModel
@{
    ViewBag.Title = "Modifier";
}

<h2>Modifier</h2>
<div class="row">
    <div class="col-sm-12">
        @using (Html.BeginForm())
        {
            <div class="form-horizontal">
                @Html.AntiForgeryToken()
                @Html.HiddenFor(m => m.Id)
                @Html.HiddenFor(m => m.IdPersonne)
                @Html.EditorFor(m => m.Personne)
                @Html.EditorForModel()
                <input type="submit" class="btn btn-default" value="Modifier" />
            </div>     
        }
    </div>
</div>
